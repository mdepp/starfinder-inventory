    "@remix-run/serve": "^2.14.0",
    "cross-env": "^7.0.3",
    "debug": "^4.3.5",
    "express": "^4.19.2",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-utils": "^7.6.0",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
